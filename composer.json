{
    "name": "catchadmin/installer",
    "description": "catchadmin 安装器.",
    "keywords": [
        "catchadmin",
        "laravel",
        "installer",
        "thinkphp",
        "webman"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JaguarJack",
            "email": "njphper@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31",
        "symfony/process": "^6.2|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/catch"
    ],
    "autoload": {
        "psr-4": {
            "CatchAdmin\\Installer\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CatchAdmin\\Installer\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
